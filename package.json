{
  "name": "property_pro",
  "version": "1.0.0",
  "description": "Property_pro application",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "prestart": "yarn migrate",
    "dev": "nodemon src/index.js",
    "build": "babel src -d dist",
    "clean": "yarn -p clean:*",
    "clean:server": "rimraf ./build",
    "lint": "eslint ./src ./test --ext .js",
    "clean:logs": "rimraf ./server/logs ./build/logs",
    "lint:fix": "eslint ./src ./test --ext .js --fix",
    "style:fix": "prettier --write 'src/**/*.js' 'test/**/*.js'",
    "migrate": "node ./src/datasource/migrations/tableCreate.js",
    "style:check": "prettier --check 'src/**/*.js' 'test/**/*.js'"
  },
  "keywords": [
    "Rent",
    "Buy",
    "Property",
    "users",
    "Advert"
  ],
  "author": "Uche Mark && Jennifer Joseph",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-pino-logger": "^7.0.0",
    "express-rate-limit": "^6.4.0",
    "http-errors": "^2.0.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.5.5",
    "ms": "^2.1.3",
    "pino": "^8.1.0"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 11"
  ],
  "devDependencies": {
    "nodemon": "^2.0.19"
  }
}
